{
  "version": 2,
  "name": "naturego-lk",
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/NatureGo.lk/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "backend/src/server.js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "VITE_API_URL": "https://naturego-lk-api.vercel.app/api"
  }
}